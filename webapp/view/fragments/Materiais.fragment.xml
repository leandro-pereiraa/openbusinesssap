<mvc:View xmlns:mvc="sap.ui.core.mvc"
xmlns="sap.m">
    <GenericTile
        class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
        header="Materiais"
        subheader="Cadastro"
        press="navToClient"
    >
        <TileContent
            unit="Unit"
            footer="Footer"
        >
            <ImageContent src="sap-icon://add"/>
        </TileContent>
    </GenericTile>
    <GenericTile
        class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
        header="Materiais"
        subheader="Lista"
        press="navToClient"
    >
        <TileContent
            unit="Unit"
            footer="Footer"
        >
            <ImageContent src="sap-icon://list"/>
        </TileContent>
    </GenericTile>
</mvc:View>