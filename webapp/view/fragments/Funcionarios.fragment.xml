<mvc:View xmlns:mvc="sap.ui.core.mvc"
xmlns="sap.m">
    <GenericTile
            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
            header="Funcionários"
            subheader="Cadastro"
            press="navToVendorCreate"
        >
            <TileContent
                unit="Unit"
                footer="Footer"
            >
                <ImageContent src="sap-icon://add"/>
            </TileContent>
        </GenericTile>
        <GenericTile
            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
            header="Funcionários"
            subheader="Lista"
            press="navToVendorList"
        >
            <TileContent
                unit="Unit"
                footer="Footer"
            >
                <ImageContent src="sap-icon://list"/>
            </TileContent>
        </GenericTile>
</mvc:View>