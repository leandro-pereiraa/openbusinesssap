{
  "_version": "1.32.0",
  "sap.app": {
    "id": "openBusiness",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "dataSources": {
      "mainService": {
        "uri": "/api",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "openBusiness.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.ui.table": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "openBusiness.i18n.i18n"
        }
      },
      "userModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
      "oCreateModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "oListModel": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "openBusiness.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteDashboard",
          "pattern": "",
          "target": [
            "TargetDashboard"
          ]
        },
        {
          "name": "Login",
          "pattern": "Login",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "Register",
          "pattern": "Register",
          "target": [
            "TargetRegister"
          ]
        },
        {
          "name": "RouteCreateCliente",
          "pattern": "RouteCreateCliente",
          "target": [
            "TargetCreateCliente"
          ]
        },
        {
          "name": "RouteListClient",
          "pattern": "RouteListClient",
          "target": [
            "TargetListClient"
          ]
        },
        {
          "name": "Calendar",
          "pattern": "Calendar",
          "target": [
            "TargetCalendar"
          ]
        }
      ],
      "targets": {
        "TargetDashboard": {
          "viewType": "XML",
          "viewName": "dashboard.Dashboard",
          "transition": "slide"
        },
        "TargetLogin": {
          "viewType": "XML",
          "viewName": "login.Login",
          "transition": "slide"
        },
        "TargetRegister": {
          "viewType": "XML",
          "viewName": "login.Register",
          "transition": "slide"
        },
        "TargetCreateCliente": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "cliente",
          "viewName": "cliente.Create"
        },
        "TargetListClient": {
          "transition": "slide",
          "viewType": "XML",
          "viewName": "cliente.List"
        },
        "TargetCalendar": {
          "viewType": "XML",
          "viewName": "calendar.Calendar"
        }
      }
    }
  }
}